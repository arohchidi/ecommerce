                                      <div class="page-list">
                                          <% if (pages > 0) { %>
                <ul class="pagination text-center">
                    <% if (current == 1) { %>
                        <li class="disabled"><a>First</a></li>
                    <% } else { %>
                        <li><a href="/filter_products/1?category=<%=category %>&?query=<%=query_param %>&?order=<%=order_param %>">First</a></li>
                    <% } %>
                    <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                    <% if (i !== 1) { %>
                        <li class="disabled"><a>...</a></li>
                    <% } %>
                    <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                        <% if (i == current) { %>
                            <li class="active"><a><%= i %></a></li>
                        <% } else { %>
                            <li><a href="/filter_products/<%= i %>?category=<%=category %>&?query=<%=query_param %>&?order=<%=order_param %>"><%= i %></a></li>
                        <% } %>
                        <% if (i == Number(current) + 4 && i < pages) { %>
                            <li class="disabled"><a>...</a></li>
                        <% } %>
                    <% } %>
                    <% if (current == pages) { %>
                        <li class="disabled"><a>Last</a></li>
                    <% } else { %>
                        <li><a href="/filter_products/<%= pages %>?category=<%=category %>&?query=<%=query_param %>&?order=<%=order_param %>">Last</a></li>
                    <% } %>
                </ul>
            <% } %>
                                          
